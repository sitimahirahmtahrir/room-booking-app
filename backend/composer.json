{
    "name": "yourname/room-booking-system",
    "description": "A simple room booking system",
    "type": "project",
    "require": {
        "slim/slim": "^4.8",
        "slim/psr7": "^1.5",
        "slim/http": "^1.2",
        "slim/exception": "^1.0",
        "php-di/php-di": "^6.3",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Controllers\\": "backend/src/controllers/",
            "Models\\": "backend/src/models/",
            "Middleware\\": "backend/src/middleware/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    }
}
